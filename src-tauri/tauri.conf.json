{
  "$schema": "https://schema.tauri.app/config/2.0.0-rc",
  "productName": "react.appKit.reown",
  "version": "0.1.0",
  "identifier": "com.react.appKit.reown",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:9222",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "react.appKit.reown",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "devtools": true
      }
    ],
    "security": {
      "csp": null,
    "devCsp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
