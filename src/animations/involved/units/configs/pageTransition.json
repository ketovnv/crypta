{
  "style": {
    "position": "absolute",
    "top": 0,
    "left": 0,
    "width": "100%",
    "height": 575
  },
  "transition": {
    "delay": 0.1,
    "type": "spring",
    "stiffness": 200,
    "damping": 100,
    "mass": 5,
    "friction": 20,
    "visible": {
      "delay": 0.1
    }
  },
  "hidden": {
    "opacity": 0,
    "height": 0,
    "rotateX": -225,
    "y": 600,
    "scale": 0.5
  },
  "visible": {
    "height": 500,
    "opacity": 1,
    "rotateX": 0,
    "y": 0
  },
  "exit": {
    "height": 0,
    "opacity": -0.5,
    "rotateX": -200,
    "y": 600,
    "scale": 0.5
  }
}
